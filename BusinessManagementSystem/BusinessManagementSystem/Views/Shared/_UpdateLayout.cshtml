        <aside class="left-sidebar" data-sidebarbg="skin5">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- Sidebar navigation-->
                @Html.Raw(File.ReadAllText(Server.MapPath("~/Theme/includes/sidebar.html")))
